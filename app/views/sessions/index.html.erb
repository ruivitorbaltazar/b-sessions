<h1>All your scheduled Sessions</h1>

<%= link_to 'New Session', new_session_path %>

<% @sessions.each do |session| %>
  <div class="session-item">
    <h2><%= session.name %></h2>
    <h3>Starts: <%= session.start_date %></h3>
    <h3>Ends: <%= session.end_date %></h3>
    <%= link_to 'Edit Session', edit_session_path(session) %>
  </div>
<% end %>
